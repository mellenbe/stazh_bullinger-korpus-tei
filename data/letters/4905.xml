				<p>
					<lb xml:id="p1z1"/>S.
					<lb xml:id="p1z2"/>Praestantissimo,
					<lb xml:id="p1z3"/>Ioanni Fabritio, cum
					<lb xml:id="p1z4"/>ecclesiae
					<lb xml:id="p1z5"/>fideli ministro, domino
					<lb xml:id="p1z6"/>et fratri suo
					<lb xml:id="p1z7"/>colendo et dilecto.
					<lb xml:id="p1z8"/>et hac
				</p>
