	<text xml:lang="">
		<body>
			<div>
				<p>
					<lb xml:id="p1z1"/>deus.
					<lb xml:id="p1z2"/>concionator quidam
					<lb xml:id="p1z3"/>aliquid scribere <placeName type="citizen_name" ref="l492" cert="high">Argentoratum</placeName>
					<lb xml:id="p1z4"/>si placet d. compater,
					<lb xml:id="p1z5"/>proficiscitur. Iam
					<lb xml:id="p1z6"/>qui divertit
					<lb xml:id="p1z7"/>ad hospitium. Rubros dominus cras
					<lb xml:id="p1z8"/>alias literas a <persName type="citizen_name" ref="p1548" cert="high">Richardo Hilles</persName> accepi,
					<lb xml:id="p1z9"/>per uxorem
					<lb xml:id="p1z10"/><persName type="citizen_name" ref="p506" cert="high">Butleri</persName>
					<lb xml:id="p1z11"/>quas in iunii
					<lb xml:id="p1z12"/>pro literis
					<lb xml:id="p1z13"/>agen.
					<lb xml:id="p1z14"/>gratias
					<lb xml:id="p1z15"/>plurimum salutat.
					<lb xml:id="p1z16"/>dicit se nihil novi habere nec ab inferioribus
					<lb xml:id="p1z17"/>ei scripseris.
					<lb xml:id="p1z18"/>Ex <placeName type="citizen_name" ref="l642" cert="high">Frisia</placeName>.
					<lb xml:id="p1z19"/>Coram <placeName type="citizen_name" ref="l2122" cert="high">Bullonia</placeName>, mutuis
					<lb xml:id="p1z20"/><placeName type="citizen_name" ref="l2464" cert="high">Galli</placeName> et <placeName type="citizen_name" ref="l830" cert="high">Angli</placeName>.
					<lb xml:id="p1z21"/>nisi
					<lb xml:id="p1z22"/>quam
					<lb xml:id="p1z23"/>urbis
					<lb xml:id="p1z24"/>quod me penitet
					<lb xml:id="p1z25"/>iudie
					<lb xml:id="p1z26"/>et futurum brevi bellum
					<lb xml:id="p1z27"/>cadunt
					<lb xml:id="p1z28"/>Caedibus
					<lb xml:id="p1z29"/>bene vale.
					<lb xml:id="p1z30"/>inter <placeName type="citizen_name" ref="l2464" cert="high">Gallum</placeName> et <placeName type="citizen_name" ref="l830" cert="high">Anglum</placeName>
					<lb xml:id="p1z31"/>S.
					<lb xml:id="p1z32"/>operus
					<lb xml:id="p1z33"/>ecclesiarum,
					<lb xml:id="p1z34"/>ecclesie
					<pb/>
					<lb xml:id="p2z1"/>ecclesiarum
					<lb xml:id="p2z2"/>illorum
				</p>
			</div>
		</body>
	</text>
