	<text xml:lang="">
		<body>
			<div>
				<p>
					<lb xml:id="p1z1"/>et de omni tuum
					<lb xml:id="p1z2"/>S.
					<lb xml:id="p1z3"/>Ante
					<lb xml:id="p1z4"/>has meas tibi redditas esse intelligas.
					<lb xml:id="p1z5"/>d.
					<lb xml:id="p1z6"/>receris
					<lb xml:id="p1z7"/>multo gratissimam.
					<lb xml:id="p1z8"/>certum habeam,
					<lb xml:id="p1z9"/>Vale. Dominus benedicat tibi tuisque laboribus. Salutant te fratres
					<lb xml:id="p1z10"/>tantis tu vobis
					<lb xml:id="p1z11"/>est
					<lb xml:id="p1z12"/>S.
					<lb xml:id="p1z13"/>quam
					<lb xml:id="p1z14"/>et singuli quid
					<lb xml:id="p1z15"/>et alios viros bonos.
					<lb xml:id="p1z16"/>d. d. Maiorem, Eberum,
					<lb xml:id="p1z17"/>doctissimo
					<lb xml:id="p1z18"/>15.
					<lb xml:id="p1z19"/>et
					<lb xml:id="p1z20"/>tu
					<lb xml:id="p1z21"/>d.
					<lb xml:id="p1z22"/>acto
					<lb xml:id="p1z23"/>iusti
					<lb xml:id="p1z24"/>Consulio
					<lb xml:id="p1z25"/>luus
					<lb xml:id="p1z26"/>d.
					<lb xml:id="p1z27"/>d. Ioannem Iossheim, Pontis
					<lb xml:id="p1z28"/>martii
					<lb xml:id="p1z29"/>gerus tuus.
					<lb xml:id="p1z30"/>B.
				</p>
			</div>
		</body>
	</text>
