	<text xml:lang="la">
		<body>
			<div>
				<p>
					<lb xml:id="p1z1"/>S.
					<lb xml:id="p1z2"/>ad hodiernum diem
					<lb xml:id="p1z3"/>usque
					<lb xml:id="p1z4"/>literas
					<lb xml:id="p1z5"/>D.
					<lb xml:id="p1z6"/>Tas
					<lb xml:id="p1z7"/>etiam.
					<lb xml:id="p1z8"/>Tertius
					<lb xml:id="p1z9"/>octo unis
					<lb xml:id="p1z10"/>qui est
					<lb xml:id="p1z11"/>a domino
					<lb xml:id="p1z12"/>nimium
					<lb xml:id="p1z13"/>quo die sororiam
					<lb xml:id="p1z14"/>15.
					<lb xml:id="p1z15"/>cuimus.
					<lb xml:id="p1z16"/>Coniustabs, redii,
					<lb xml:id="p1z17"/>Philippus.
					<lb xml:id="p1z18"/>regem
					<lb xml:id="p1z19"/>hoc novi attulit
					<lb xml:id="p1z20"/>ad agendum
					<lb xml:id="p1z21"/>Utrinus.
					<lb xml:id="p1z22"/>legati
					<lb xml:id="p1z23"/>postulare,
					<lb xml:id="p1z24"/>sumus
					<lb xml:id="p1z25"/>nos non
					<lb xml:id="p1z26"/>pace mittantur.
					<lb xml:id="p1z27"/>a
					<lb xml:id="p1z28"/>Tuus.
					<lb xml:id="p1z29"/>octavo vel nono
					<lb xml:id="p1z30"/>aspernati
					<lb xml:id="p1z31"/>castra in legati
					<lb xml:id="p1z32"/>Inter utraque
					<lb xml:id="p1z33"/>mensum
					<lb xml:id="p1z34"/>olim. Pace
					<lb xml:id="p1z35"/>cum
					<lb xml:id="p1z36"/>Et sane
					<lb xml:id="p1z37"/>congrudientur.
					<lb xml:id="p1z38"/>est
					<lb xml:id="p1z39"/>aliqua spios
					<lb xml:id="p1z40"/>d.
					<lb xml:id="p1z41"/>literarum
					<lb xml:id="p1z42"/>caldum, colendissime,
					<lb xml:id="p1z43"/>d.
					<pb/>
					<lb xml:id="p2z1"/>omnes Iesanniones
					<lb xml:id="p2z2"/>literarum
					<lb xml:id="p2z3"/>d.
					<lb xml:id="p2z4"/>d.
				</p>
			</div>
		</body>
