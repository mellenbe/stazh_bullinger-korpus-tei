					<lb xml:id="p1z1"/>1550.
					<lb xml:id="p1z2"/>1.
					<lb xml:id="p1z3"/>noch dennst
					<lb xml:id="p1z4"/>Franciscus Christus
					<lb xml:id="p1z5"/>d.
					<lb xml:id="p1z6"/>S.
					<lb xml:id="p1z7"/>von Erendern.
					<lb xml:id="p1z8"/>in
					<lb xml:id="p1z9"/>alles sy
					<lb xml:id="p1z10"/>esse etc.
					<lb xml:id="p1z11"/>Schrumpettus.
					<lb xml:id="p1z12"/>per
					<lb xml:id="p1z13"/>et
					<lb xml:id="p1z14"/>d.
					<lb xml:id="p1z15"/>furgerie piorum
					<lb xml:id="p1z16"/>hett Berger.
					<lb xml:id="p1z17"/>d.
					<lb xml:id="p1z18"/>d.
					<lb xml:id="p1z19"/>d.
					<lb xml:id="p1z20"/>f.
					<lb xml:id="p1z21"/>1.
					<lb xml:id="p1z22"/>willen, die wort wider ett
					<lb xml:id="p1z23"/>S.
					<lb xml:id="p1z24"/>et
					<lb xml:id="p1z25"/>minem
					<lb xml:id="p1z26"/>Vesteri
					<lb xml:id="p1z27"/>kirten,
					<lb xml:id="p1z28"/>d.
					<lb xml:id="p1z29"/>p&#252;nstten, die will aber
					<lb xml:id="p1z30"/>r&#252;w gott, mine lieben wider
					<lb xml:id="p1z31"/>S.
					<lb xml:id="p1z32"/>in miner herst haben von
					<lb xml:id="p1z33"/>genein
					<lb xml:id="p1z34"/>unwand.
					<lb xml:id="p1z35"/>Heych, qui der versatz
					<lb xml:id="p1z36"/>Et ainer
					<lb xml:id="p1z37"/>er vlys uff
					<lb xml:id="p1z38"/>et gegen profen leut
					<lb xml:id="p1z39"/>&#220;wer etc.
					<lb xml:id="p1z40"/>mihi
					<lb xml:id="p1z41"/>Hatt von se
					<lb xml:id="p1z42"/>an griffen.
					<lb xml:id="p1z43"/>namen lieben
