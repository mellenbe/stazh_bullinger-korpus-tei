				<zone xml:id="p4z43-0" points="3297,4425 2531,4422 2336,4423 912,4475 914,4525 2337,4473 2532,4472 3296,4475">914,4513 2337,4461 2532,4460 3297,4463</zone>
				<zone xml:id="p4z44-0" points="3345,4570 2731,4525 1363,4555 909,4600 914,4650 1366,4605 2730,4575 3342,4620">913,4638 1366,4593 2731,4563 3343,4608</zone>
				<zone xml:id="p4z45-0" points="3247,4668 2879,4655 2834,4669 2802,4680 2793,4669 2756,4704 2788,4737 2878,4708 2886,4705 3245,4718">2766,4696 2792,4723 2885,4693 3246,4706</zone>
