					<lb xml:id="p1z1"/>Duas 12. aprilis et 10. maii literas scripsisti
					<lb xml:id="p1z2"/>S D.
					<lb xml:id="p1z3"/>accepi et protinum
					<lb xml:id="p1z4"/>honorande et frater charissime.
					<lb xml:id="p1z5"/>aliis etiam per Helvetiam ecclesiarum pastoribus
					<lb xml:id="p1z6"/>d.
					<lb xml:id="p1z7"/>obsecrans, ut et assiduas preces in
					<lb xml:id="p1z8"/>responderent, ac
					<lb xml:id="p1z9"/>suis ad illustrissimi principis, compater
					<lb xml:id="p1z10"/>copis Condensis et foederitis
					<lb xml:id="p1z11"/>illustrissimo
					<lb xml:id="p1z12"/>optimum
					<lb xml:id="p1z13"/>commendarent, iuveni omnes
					<lb xml:id="p1z14"/>eius suis fideliter
					<lb xml:id="p1z15"/>deratorum
					<lb xml:id="p1z16"/>imparatissimos. Omnes enim et precari pro vobis et
					<lb xml:id="p1z17"/>magistratibus alioqui faventibus ex animo
					<lb xml:id="p1z18"/>coenim
					<lb xml:id="p1z19"/>commendare non desinimus. Quid vero apud
					<lb xml:id="p1z20"/>nostros impetraverimus, intellexisse te iam puto,
					<lb xml:id="p1z21"/>ex scorteres, qui
					<lb xml:id="p1z22"/>partim ex Derlacho partim
					<lb xml:id="p1z23"/>paterque comitiis interfuitur de mittendo et praecionibus
					<lb xml:id="p1z24"/>posterioribus. Nunc agitur de mittenda legatione
					<lb xml:id="p1z25"/>solicitus. Exceris
					<lb xml:id="p1z26"/>ad vos. Multum autem scio, quid
					<lb xml:id="p1z27"/>verientia enim edoctus, scio, quid compositiones pacis
					<lb xml:id="p1z28"/>et religionibus
					<lb xml:id="p1z29"/>regiis regionibus
					<lb xml:id="p1z30"/>iamus, cum ex
					<lb xml:id="p1z31"/>diversa ingenia ad componendum confluunt. Nihil
					<lb xml:id="p1z32"/>benefuit nobis in nostro illo infelici bello gesto ante
					<lb xml:id="p1z33"/>principio enim impedierit,
					<lb xml:id="p1z34"/>Petius. Primum
					<lb xml:id="p1z35"/>S.
					<lb xml:id="p1z36"/>annos 30, vehementer
					<lb xml:id="p1z37"/>vestri, ne proficiscerentur in hostem nondum satis
					<lb xml:id="p1z38"/>esset, nussoiam
					<lb xml:id="p1z39"/>instructum. Ubi hic satis in
					<lb xml:id="p1z40"/>res nostrae iam
					<lb xml:id="p1z41"/>apparebant compositores.
					<lb xml:id="p1z42"/>aderat compositores, quibus authoritate
					<lb xml:id="p1z43"/>etc.
					<lb xml:id="p1z44"/>et
					<lb xml:id="p1z45"/>reliquae et
					<lb xml:id="p1z46"/>pacis conditiones inductae dunt
					<lb xml:id="p1z47"/>S.
					<lb xml:id="p1z48"/>Domino
					<lb xml:id="p1z49"/>quoque hodie adhuc, qui
					<lb xml:id="p1z50"/>de quibus optimis
					<lb xml:id="p1z51"/>ad
					<lb xml:id="p1z52"/>tuus potes
					<lb xml:id="p1z53"/>oro ex animo, ut illustrissimo princeps diu
					<lb xml:id="p1z54"/>sententiam veram solidam et stabilem pacem regno
					<lb xml:id="p1z55"/>composito.
					<lb xml:id="p1z56"/>florentissimum restuturam, nescio, qualem
					<lb xml:id="p1z57"/>vestram nescio qualem inducant. Dubium
					<lb xml:id="p1z58"/>res
					<lb xml:id="p1z59"/>non est, quin Wirtembergensi et similes eius
					<lb xml:id="p1z60"/>conaturi sint et confessionem Augustanam Galliae commendationem
					<lb xml:id="p1z61"/>vero nosti quid illa
					<lb xml:id="p1z62"/>pepere.
					<lb xml:id="p1z63"/>Tuus
					<lb xml:id="p1z64"/>et quae hodie adhuc concertationes ex illa
					<lb xml:id="p1z65"/>existant. Si ergo locus ei dabitur in Gallia certam
					<lb xml:id="p1z66"/>S.
					<lb xml:id="p1z67"/>erit datum.
					<lb xml:id="p1z68"/>interminabilibus
					<lb xml:id="p1z69"/>S.
					<lb xml:id="p1z70"/>singolare. Gratias
					<lb xml:id="p1z71"/>diligenter
